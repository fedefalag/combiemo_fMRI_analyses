{
  "Name": "Combiemo Auditory",
  "Description": "",
  "Input": {
    "task": "voicelocalizerCombiemo"
  },
  "Steps": [
    {
      "Level": "run",
      "Model": {
        "X": [
          "trial_type.voices",
          "trial_type.objects",
          "trial_type.keypress",
          "trans_x", "trans_y", "trans_z", "rot_x", "rot_y", "rot_z"
        ]
      }
    },
    {
      "Level": "subject",
      "Model": {
        "X": [
          "trial_type.voices",
          "trial_type.objects",
          "trial_type.keypress",
          "trans_x", "trans_y", "trans_z", "rot_x", "rot_y", "rot_z"
        ]
      },
      "AutoContrasts": ["trial_type.voices", "trial_type.objects" ],
      "Contrasts": [
        {
          "Name": "voices_gt_objects",
          "ConditionList": [
            "trial_type.voices", "trial_type.objects"
          ],
          "weights": [1, -1],
          "type": "t"
        },
        {
          "Name": "objects_gt_voices",
          "ConditionList": [
            "trial_type.voices", "trial_type.objects"
          ],
          "weights": [-1, 1],
          "type": "t"
        },
        {
          "Name": "all_gt_baseline",
          "ConditionList": [
            "trial_type.voices", "trial_type.objects"
          ],
          "weights": [1, 1],
          "type": "t"
        }
      ]
    },
    {
      "Level": "dataset",
      "AutoContrasts": [
        "trial_type.voices",
        "trial_type.objects",
        "voices_gt_objects",
        "objects_gt_voices"]
    }
  ]
}
